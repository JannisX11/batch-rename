{
  "name": "batch-rename-extension",
  "displayName": "Batch Rename",
  "description": "Batch rename files in the explorer inside a text editor",
  "version": "0.0.6",
  "publisher": "JannisX11",
  "repository": {
		"type": "git",
		"url": "https://github.com/JannisX11/batch-rename"
  },
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.renameBatch"
  ],
  "icon": "icon.png",
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.renameBatch",
        "title": "Batch Rename",
        "category": "Batch Renamer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "!explorerResourceIsFolder",
          "group": "7_modification",
          "command": "extension.renameBatch"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.5",
    "mocha": "^5.0.0",
    "typescript": "^2.7.1",
    "vscode": "^1.1.10"
  },
  "dependencies": {
    "opn": "^5.2.0",
    "path": "^0.12.7"
  },
  "license": "MIT",
  "keywords": [
    "rename",
    "multiple",
    "extension",
    "vscode"
  ]
}
